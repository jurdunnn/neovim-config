{
    "codeLens.enable": true,
    "codeLens.position": "eol",
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.formatOnType": false,
    "coc.preferences.formatOnSave": false,
    "coc.preferences.willSaveHandlerTimeout": 1000,
    "coc.preferences.enableLinkedEditing": true,
    "phpstan.enable": false,
    "phpstan.level": 2,
    "phpstan.neonLint.enable": false,
    "intelephense.environment.phpVersion": "8.2.0",
    "intelephense.environment.includePaths": [
        "vendor"
    ],
    "intelephense.files.maxSize": 5000000,
    "intelephense.stubs": [
        "apache", "bcmath", "bz2", "calendar", "com_dotnet", "Core", "ctype", "curl", "date",
        "dba", "dom", "enchant", "exif", "FFI", "fileinfo", "filter", "fpm", "ftp", "gd",
        "gettext", "gmp", "hash", "iconv", "imap", "intl", "json", "ldap", "libxml", "mbstring",
        "meta", "mysqli", "oci8", "odbc", "openssl", "pcntl", "pcre", "PDO", "pdo_ibm",
        "pdo_mysql", "pdo_pgsql", "pdo_sqlite", "pgsql", "Phar", "posix", "pspell", "readline",
        "Reflection", "session", "shmop", "SimpleXML", "snmp", "soap", "sockets", "sodium",
        "SPL", "sqlite3", "standard", "superglobals", "sysvmsg", "sysvsem", "sysvshm", "tidy",
        "tokenizer", "xml", "xmlreader", "xmlrpc", "xmlwriter", "xsl", "Zend OPcache", "zip", "zlib"
    ],
    "intelephense.format.enable": false,
    "intelephense.completion.insertUseDeclaration": true,
    "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": false,
    "intelephense.diagnostics.run": "onType",
    "intelephense.phpdoc.returnVoid": false,
    "intelephense.trace.server": "off",
    "intelephense.completion.triggerParameterHints": true,
    "intelephense.completion.maxItems": 100,
    "prettier.disableLanguages": ["html"],
    "tailwindCSS.headwind.runOnSave": false,
    "php-cs-fixer.enable": true,
    "php-cs-fixer.executablePath": "php-cs-fixer",
    "php-cs-fixer.onsave": false,
    "php-cs-fixer.rules": "@PSR12",
    "php-cs-fixer.allowRisky": false,
    "php-cs-fixer.pathMode": "override",
    "php-cs-fixer.exclude": [],
    "emmet.includeLanguages": {
        "blade": "html",
        "vue": "html",
        "php": "html"
    },
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "html.suggest.html5": true,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "suggest.noselect": false,
    "suggest.completionItemKindLabels": {
	  "text": "text",
	  "method": "method",
	  "function": "function",
	  "constructor": "constructor",
	  "field": "field",
	  "variable": "variable",
	  "class": "class",
	  "interface": "interface",
	  "module": "module",
	  "property": "property",
	  "unit": "unit",
	  "value": "value",
	  "enum": "enum",
	  "keyword": "keyword",
	  "snippet": "snippet",
	  "color": "color",
	  "file": "file",
	  "reference": "reference",
	  "folder": "folder",
	  "enumMember": "enum member",
	  "constant": "constant",
	  "struct": "struct",
	  "event": "event",
	  "operator": "operator",
	  "typeParameter": "type",
	  "default": "default"
	},
    "suggest.labelMaxLength": 50,
    "headwind.runOnSave": false,
    "diagnostic.format": "%message [%source]",
    "diagnostic.virtualText": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.separateRelatedInformationAsDiagnostics": true,
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "signature.target": "echo",
    "suggest.detailField": "abbr",
    "suggest.defaultSortMethod": "none",
    "suggest.selection": "recentlyUsedByPrefix",
    "coc.source.around.enable": true,
    "coc.source.buffer.enable": true,
    "coc.source.file.enable": true,
    "extensions.autoInstall": true,
    "extensions.autoUpdate": true,
    "languageserver": {},
    "coc.preferences.enableLanguageSourcePriority": true,
    "coc.source.around.priority": 1,
    "coc.source.buffer.priority": 2,
    "coc.source.file.priority": 3,
    "coc.source.word.priority": 0,
    "html.enable": true,
    "html.filetypes": ["html", "handlebars", "blade", "php"],
    "emmet.enable": true,
    "emmet.priority": 10,
    "css.enable": true,
    "css.filetypes": ["css", "scss", "less", "postcss"],
    "tailwindCSS.enable": true,
    "tailwindCSS.filetypes": ["html", "css", "scss", "javascript", "typescript", "vue", "blade", "php"],
    "blade.enable": true,
    "phpls.enable": true,
    "phpls.filetypes": ["php"],
    "signature.floatConfig": {
        "title": " Signature",
        "highlight": "CocSymbol",
        "border": true
    },
    "diagnostic.floatConfig": {
        "title": " Diagnostics",
        "highlight": "CocSymbol",
        "border": true
    },
    "hover.floatConfig": {
        "title": " Description",
        "highlight": "CocSymbol",
        "border": true
    },
    "suggest.floatConfig": {
        "title": " Suggest",
        "highlight": "CocSymbol",
        "border": true
    }
}
